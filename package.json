  "main": "dist/index",
  "typing": "dist/index",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "lint": "tslint ./src/**.ts",
    "prebuild": "npm run lint && rimraf ./dist",
    "build": "tsc",
    "watch:build": "tsc --watch"
  },
  "author": "Olivia Trewin",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@types/node": "^10.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrionNebula/event-filter.git"
  },
  "keywords": [
